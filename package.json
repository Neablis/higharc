{
  "name": "higharc",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "node ./lib/index.js",
    "build": "npm run-script build:js && npm run-script build:types",
    "build:watch": "npm run-script build:js -- --watch && npm run-script build:types",
    "build:js": "babel --source-maps --extensions '.ts,.tsx,.js,.jsx' --copy-files --no-copy-ignored --delete-dir-on-start src -d lib --ignore **/*.stories.js,**/*.test.js,**/*.mock.js",
    "build:types": "tsc --declaration --outDir lib/ --emitDeclarationOnly --declarationMap",
    "dev": "yarn type-check:watch & yarn build:watch & nodemon ./lib/index.js",
    "test": "npx jest",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npx typeorm migration:generate",
    "migration:run": "npx typeorm migration:run",
    "migration:revert": "npx typeorm migration:revert",
    "migration:drop": "npx typeorm schema:drop",
    "drop": "npm run-script migration:drop && npm run-script migration:run",
    "lint": "eslint . --config './.eslintrc.js' --ext .ts"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@types/express": "^4.17.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "type-graphql-filter": "^0.2.2",
    "typeorm": "^0.2.29"
  }
}
