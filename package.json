{
  "name": "higharc",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "graphql-markdown": "^5.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "node ./lib/index.js",
    "build": "npm run-script build:js && npm run-script build:types",
    "build:watch": "npm run-script build:js -- --watch && npm run-script build:types",
    "build:js": "babel --source-maps --extensions '.ts,.tsx,.js,.jsx' --copy-files --no-copy-ignored --delete-dir-on-start src -d lib --ignore **/*.stories.js,**/*.test.js,**/*.mock.js",
    "build:types": "tsc --declaration --outDir lib/ --emitDeclarationOnly --declarationMap",
    "dev": "yarn type-check:watch & yarn build:watch & nodemon ./lib/index.js",
    "test": "npx jest",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npx typeorm migration:generate",
    "migration:run": "npx typeorm migration:run",
    "migration:revert": "npx typeorm migration:revert",
    "generate:schemadoc": "graphql-markdown --title 'Higharc Smoothie API' --prologue 'Description of the API' http://localhost:5000/graphql > README.md"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@types/express": "^4.17.9",
    "apollo-server-express": "^2.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "graphql-tag": "^2.11.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "type-graphql-filter": "^0.2.2",
    "typeorm": "^0.2.29"
  }
}
